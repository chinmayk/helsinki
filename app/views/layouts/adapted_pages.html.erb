		</div>
	<% end %>
	<div id="page-content">
    <%= yield %>
  </div>
	<div data-role="footer">
		<h4>Page Footer</h4>
	</div><!-- /header -->
</div><!-- /page -->



	<!-- /content -->

	<div data-role="footer">
		<h4>Page Footer</h4>
	</div><!-- /header -->
</div>
</body>
</html>	